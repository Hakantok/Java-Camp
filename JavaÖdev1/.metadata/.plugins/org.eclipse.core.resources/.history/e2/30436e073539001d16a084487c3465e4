package sesliHarfler;

public class Main {

	public static void main(String[] args) {
		
		char harf = 'k';
		
		String[] inceSesliHarf = {"e","i","ö","ü"};
		String[] kalýnSesliHarf = {"a","ý","o","u"};
		
		for(int i=0; i < inceSesliHarf.length; i++) {
			for(String inceSesli : inceSesliHarf ) {
				if(inceSesli == inceSesliHarf[i]) {
					System.out.println(harf + " : Ýnce sesli bir harftir.");
					return;
				}
			}
		}
		
		for(int i=0; i < kalýnSesliHarf.length; i++) {
			for(String kalýnSesli : kalýnSesliHarf ) {
				if(kalýnSesli == kalýnSesliHarf[i]) {
					System.out.println("Kalýn sesli bir harftir.");
					return;
				}
			}
		}
		
		System.out.println("Bu sesli bir harf deðildir.");

	}

}
